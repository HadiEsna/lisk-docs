{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"target": "es2019",
		"module": "commonjs",
		"declaration": true,
		"moduleResolution": "node",
		"newLine": "lf",
		"pretty": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"sourceMap": true,
                "skipLibCheck": true
	},  
       "typedocOptions": {
                "entryPoints": ["framework/src/modules/token"],
                "exclude": ["**/test/**"],
                "entryPointStrategy": "resolve",
                "out": "docs"
        }
}
